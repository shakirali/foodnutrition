# Default LLM Provider (e.g., openai, anthropic, etc.)
DEFAULT_LLM_PROVIDER=openai

# Default LLM API Key (fallback to OPENAI_API_KEY if not set)
DEFAULT_LLM_API_KEY=your-api-key-here
# Or use OPENAI_API_KEY as fallback
# OPENAI_API_KEY=your-api-key-here

# Default LLM Temperature (0.0 to 2.0, default: 0.7)
DEFAULT_LLM_TEMPERATURE=0.7

# Google API Configuration (for local store search)
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_SEARCH_ENGINE_ID=your-search-engine-id-here

DEFAULT_LLM_MODEL=your-llm-model